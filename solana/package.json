    "test": "ts-mocha -p ./tsconfig.mocha.json ./test/*.spec.ts --timeout 60000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coin98/coin98-dollar-mint-burn.git"
  },
