    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@coin98/chainlink-dfeed-js": "^0.1.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "mocha": "^10.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
